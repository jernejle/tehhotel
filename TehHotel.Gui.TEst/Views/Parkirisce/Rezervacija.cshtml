@model TehHotel.Gui.Test.Models.RezervacijaPosebneStoritve
@{
    ViewBag.Title = "Rezervacija";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Rezervacija parkirisca</h2>
<br />

<div class="vsebina">
    @using (Html.BeginForm("MozneRezervacije", "Parkirisce", FormMethod.Post))
    {
        @:Hotel
        @Html.DropDownListFor(m => m.idStoritve, (List<SelectListItem>)ViewData["hoteli"])<br />
        @:Datum Od 
        @Html.TextBoxFor(m => m.datumOd, new { @class = "datepicker" })
        <br />
        @:Datum Do
        @Html.TextBoxFor(m => m.datumDo, new { @class = "datepicker" })
        <br />
        <input type="submit" name="submit" value="Išči" />
    }
</div>
